{
  "name": "@alexaegis/vitest",
  "description": "My vitest config",
  "version": "0.8.4",
  "license": "MIT",
  "private": false,
  "archetype": {
    "platform": "node",
    "framework": "vitest",
    "language": "ts",
    "kind": "lib"
  },
  "keywords": [
    "managed-by-autotool",
    "vitest"
  ],
  "author": {
    "email": "alexaegis@gmail.com",
    "name": "Alex Aegis",
    "url": "https://github.com/AlexAegis"
  },
  "homepage": "https://github.com/AlexAegis/js-tooling",
  "repository": {
    "url": "git+https://github.com/AlexAegis/js-tooling",
    "type": "git",
    "directory": "packages/vitest"
  },
  "bugs": {
    "email": "alexaegis@gmail.com",
    "url": "https://github.com/AlexAegis/js-tooling/issues"
  },
  "type": "module",
  "config": {
    "engine-strict": true
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.10.0",
    "pnpm": ">=8.0.0"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs",
      "default": "./index.js"
    },
    "./readme": "./readme.md",
    "./vitest-node.config": {
      "types": "./vitest-node.config.d.ts",
      "import": "./vitest-node.config.js",
      "require": "./vitest-node.config.cjs",
      "default": "./vitest-node.config.js"
    },
    "./vitest-svelte.config": {
      "types": "./vitest-svelte.config.d.ts",
      "import": "./vitest-svelte.config.js",
      "require": "./vitest-svelte.config.cjs",
      "default": "./vitest-svelte.config.js"
    },
    "./vitest-web.config": {
      "types": "./vitest-web.config.d.ts",
      "import": "./vitest-web.config.js",
      "require": "./vitest-web.config.cjs",
      "default": "./vitest-web.config.js"
    }
  },
  "dependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.5.2",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "jsdom": "22.1.0",
    "vitest": "^0.34.6"
  },
  "devDependencies": {
    "@alexaegis/vite": "^0.8.4",
    "publint": "^0.2.5",
    "vite": "^4.5.0"
  },
  "scripts": {
    "build": "turbo run build-lib_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vitest",
    "build-lib_": "vite build",
    "lint:depcheck": "turbo run lint:depcheck_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vitest",
    "lint:depcheck_": "depcheck",
    "lint:es": "turbo run lint:es_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vitest",
    "lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
    "lint:format": "turbo run lint:format_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vitest",
    "lint:format_": "prettier --cache-location .cache/prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check .",
    "lint:md": "turbo run lint:md_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vitest",
    "lint:md_": "remark --frail --no-stdout --silently-ignore .",
    "lint:tsc": "turbo run lint:tsc_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vitest",
    "lint:tsc_": "tsc --noEmit",
    "publint": "BUILD_REASON='publish' turbo run publint_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vitest",
    "publint_": "publint dist",
    "all": "BUILD_REASON='publish' turbo run all_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vitest",
    "format": "turbo run format_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vitest",
    "format_": "prettier --cache-location .cache/prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write .",
    "test": "turbo run test_ --concurrency 16 --cache-dir .cache/turbo --filter @alexaegis/vitest",
    "test_": "vitest --passWithNoTests --coverage --run",
    "test:watch": "vitest --passWithNoTests --coverage"
  }
}
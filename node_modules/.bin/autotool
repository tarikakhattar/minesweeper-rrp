#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tarika/Library/CloudStorage/OneDrive-Personal/minesweeper/node_modules/.pnpm/autotool@0.3.0/node_modules/autotool/bin/node_modules:/Users/tarika/Library/CloudStorage/OneDrive-Personal/minesweeper/node_modules/.pnpm/autotool@0.3.0/node_modules/autotool/node_modules:/Users/tarika/Library/CloudStorage/OneDrive-Personal/minesweeper/node_modules/.pnpm/autotool@0.3.0/node_modules:/Users/tarika/Library/CloudStorage/OneDrive-Personal/minesweeper/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tarika/Library/CloudStorage/OneDrive-Personal/minesweeper/node_modules/.pnpm/autotool@0.3.0/node_modules/autotool/bin/node_modules:/Users/tarika/Library/CloudStorage/OneDrive-Personal/minesweeper/node_modules/.pnpm/autotool@0.3.0/node_modules/autotool/node_modules:/Users/tarika/Library/CloudStorage/OneDrive-Personal/minesweeper/node_modules/.pnpm/autotool@0.3.0/node_modules:/Users/tarika/Library/CloudStorage/OneDrive-Personal/minesweeper/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../autotool/bin/autotool.js" "$@"
else
  exec node  "$basedir/../autotool/bin/autotool.js" "$@"
fi
